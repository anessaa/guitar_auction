
<h1>LOG IN</h1>
<id="alert"><%= alert %></p>

<%= form_tag sessions_path do %>
  <div>
    <%= label_tag :email %>
    <%= text_field_tag :email, params[:email] %>
  </div>
  <div>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <br>
  <div><%= submit_tag "Log In" %></div>

<% end %>